<UserControl x:Class="SofomoWpfApp1.Components.IpAdresReportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SofomoWpfApp1.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="mainGrid">
        <TextBlock Name="jsonContent" Margin="0,105,0,0">

        </TextBlock>
        <StackPanel Margin="0,0,150,0" Height="100" VerticalAlignment="Top">
            <Label FontSize="16" Content="country" Name="country" />
            <Label Content="region" Name="region" />
            <Label FontSize="16" Content="city" Name="city" />
        </StackPanel>
    </Grid>
</UserControl>
